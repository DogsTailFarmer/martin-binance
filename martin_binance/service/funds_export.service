[Unit]
Description=Crypto Exporter
After=syslog.target
After=network.target

[Service]
Type=simple
User=ubuntu
Group=sudo
WorkingDirectory=/home/ubuntu/.local/lib/python3.8/site-packages/martin_binance/
ExecStart=python3 funds_rate_exporter.py
RestartSec=10
Restart=always

[Install]
WantedBy=multi-user.target
